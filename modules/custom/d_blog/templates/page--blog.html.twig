{#
/**
 * @file
 * Theme override to display a blog list view.
 *.
 * @see template_preprocess_page()
 */
#}

{% extends "page.html.twig" %}

{% block content %}
  <div{{ main_attributes.addClass('container-fluid').removeClass('container', 'py-4') }}>
    <div class="row clearfix">
      {{ page.breadcrumb }}
      <main class="main-content">
        <h1 class="blog-listing-main-header">{{ page['#title'] }} </h1>
        <section class="section">
          <a id="main-content" tabindex="-1"></a>
          {{ page.content }}
        </section>
      </main>
    </div>
  </div>
{% endblock %}
